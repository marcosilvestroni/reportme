{
  "name": "reportme",
  "description": "Reporting application for crm Method4",
  "version": "0.1.0",
  "author": "Marco Silvestroni",
  "private": true,
  "dependencies": {
    "cors": "^2.8.5",
    "electron-is-dev": "^1.1.0",
    "express": "^4.17.1",
    "fast-csv": "^3.4.0",
    "fs": "^0.0.1-security",
    "mssql": "^5.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1",
    "winreg": "^1.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"nodemon ./src/_server/app.js\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder --win --ia32 --x64 ",
    "preelectron-pack": "yarn build",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "nodemon": "^1.19.2",
    "wait-on": "^3.3.0"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.electron.deskapp",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
